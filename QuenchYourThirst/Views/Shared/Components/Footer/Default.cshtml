@using QuenchYourThirst.Models
@model IList<footer>
<footer class="ftco-footer ftco-section">
    <div class="container">
        <div class="row">
            <div class="mouse">
                <a href="#" class="mouse-icon">
                    <div class="mouse-wheel"><span class="ion-ios-arrow-up"></span></div>
                </a>
            </div>
        </div>
        <div class="row mb-5">
            <div class="col-md">
                <div class="ftco-footer-widget mb-4">
                    <h2 class="ftco-heading-2">QYTHIRST</h2>
                    @foreach(var item in Model)
                    {
                        <p>@item.QYTHIRST</p>
                    }
                    <ul class="ftco-footer-social list-unstyled float-md-left float-lft mt-5">
                        <li class="ftco-animate"><a href="#"><span class="icon-twitter"></span></a></li>
                        <li class="ftco-animate"><a href="#"><span class="icon-facebook"></span></a></li>
                        <li class="ftco-animate"><a href="#"><span class="icon-instagram"></span></a></li>
                    </ul>
                </div>
            </div>
            <div class="col-md">
                <div class="ftco-footer-widget mb-4 ml-md-5">
                    <h2 class="ftco-heading-2">Menu</h2>
                    @foreach(var item in Model)
                    {
                        <ul class="list-unstyled">
                            <li><a href="#" class="py-1 d-block">@item.Menu</a></li>
                        </ul>
                    }
                    @* <ul class="list-unstyled">
                        <li><a href="#" class="py-2 d-block">Shop</a></li>
                        <li><a href="#" class="py-2 d-block">About</a></li>
                        <li><a href="#" class="py-2 d-block">Journal</a></li>
                        <li><a href="#" class="py-2 d-block">Contact Us</a></li>
                    </ul> *@
                </div>
            </div>
            <div class="col-md-4">
                <div class="ftco-footer-widget mb-4">
                    <h2 class="ftco-heading-2">Help</h2>
                    @foreach(var item in Model)
                    {
                    <div class="d-flex">
                            <div class="d-flex">
                        <ul class="list-unstyled mr-l-5 pr-l-3 mr-4">
                            <li><a href="#" class="py-1 d-block">@item.Help</a></li>
                           
                          @*  <li><a href="#" class="py-2 d-block">Returns &amp; Exchange</a></li>
                            <li><a href="#" class="py-2 d-block">Terms &amp; Conditions</a></li>
                            <li><a href="#" class="py-2 d-block">Privacy Policy</a></li>  *@
                        </ul>
                         </div>
                            <ul class="list-unstyled mr-l-5 pr-l-3 mr-4">
                            <li><a href="#" class="py-1 d-block">@item.Help2</a></li>
                            @* <li><a href="#" class="py-2 d-block">Contact</a></li> *@
                        </ul>
                    </div>
                    }
                </div>
            </div>
            <div class="col-md">
                <div class="ftco-footer-widget mb-4">
                    <h2 class="ftco-heading-2">Liên hệ</h2>
                    @foreach(var item in Model)
                    {
                    <div class="block-23 mb-3">
                        <ul>
                           @*  <li><span class="icon icon-map-marker"></span><span class="text">203 Fake St. Mountain View, San Francisco, California, USA</span></li>
                            <li><a href="#"><span class="icon icon-phone"></span><span class="text">+2 392 3929 210</span></a></li> *@
                            <li><a href="#"><span class="@item.Icon"></span><span class="text"> @item.Have_a_Question</span></a></li>
                        </ul>
                    </div>
                    }
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-12 text-center">
                @foreach(var item in Model)
                {
                    <p>
                        <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
                        @item.Title
                        <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
